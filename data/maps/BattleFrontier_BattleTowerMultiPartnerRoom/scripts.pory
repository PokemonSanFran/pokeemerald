const ATTENDANT = 1
const PLAYER = OBJ_EVENT_ID_PLAYER

const VAR_HEARD_INSTRUCTIONS= VAR_TEMP_1
const VAR_CHOSEN_PARTNER_ID= VAR_TEMP_2
const VAR_LEAVE_ROOM= VAR_TEMP_3

mapscripts BattleFrontier_BattleTowerMultiPartnerRoom_MapScripts{
    MAP_SCRIPT_ON_RESUME{
        pyramid_resetparty
        end
    }
    MAP_SCRIPT_ON_TRANSITION{
        if (flag(FLAG_CHOSEN_MULTI_BATTLE_NPC_PARTNER)){
            goto(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_ChosePartner)
        }

        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_1)
        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_2)
        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_3)
        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_4)
        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_5)
        clearflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_6)

        setflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_ALT_1)
        setflag(FLAG_HIDE_BATTLE_TOWER_MULTI_BATTLE_PARTNER_ALT_2)

        tower_loadpartners
        end
    }
    MAP_SCRIPT_ON_WARP_INTO_MAP_TABLE[
        VAR_LEAVE_ROOM, 1: BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_TurnPlayerNorth
    ]
    MAP_SCRIPT_ON_FRAME_TABLE[
        VAR_HEARD_INSTRUCTIONS, 0: BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_EnterRoom
        VAR_LEAVE_ROOM, 1: BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_ExitRoom
    ]
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_ChosePartner{
    setvar(VAR_HEARD_INSTRUCTIONS, 1)
    setvar(VAR_LEAVE_ROOM, 1)

    setobjectxyperm(ATTENDANT, 10, 2)
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_TurnPlayerNorth{
	turnobject(PLAYER, DIR_NORTH)
    end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_ExitRoom{
	lockall
	setvar(VAR_LEAVE_ROOM, 0)
	goto(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_EnterElevator)
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_EnterRoom{
	lockall
	applymovement(PLAYER, BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PlayerEnterRoom)
	waitmovement(0)

	applymovement(ATTENDANT, BattleFrontier_BattleTowerMultiPartnerRoom_Movement_AttendantBlockExit)
	waitmovement(0)
	copyobjectxytoperm(ATTENDANT)

	applymovement(PLAYER, Common_Movement_WalkInPlaceFasterUp)
	waitmovement(0)

	msgbox(BattleFrontier_BattleTowerMultiPartnerRoom_Text_PleaseFindPartner, MSGBOX_DEFAULT)

	special(HealPlayerParty)
	setvar(VAR_HEARD_INSTRUCTIONS, 1)
	releaseall
	end
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PlayerEnterRoom{
	walk_down
	step_end
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_AttendantBlockExit{
	walk_left
	walk_in_place_faster_down
	step_end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_Attendant{
	lock
	faceplayer

	message(BattleFrontier_BattleTowerMultiPartnerRoom_Text_QuitLookingForPartner)
	waitmessage

	multichoicedefault(20, 8, MULTI_YESNO, 1, FALSE)
    if (var(VAR_RESULT) == NO){
        goto(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_QuitChallenge)
    }

	msgbox(BattleFrontier_BattleTowerMultiPartnerRoom_Text_PleaseFindPartner2, MSGBOX_DEFAULT)

	release
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_QuitChallenge{
	frontier_set(FRONTIER_DATA_RECORD_DISABLED, TRUE)
	goto(BattleFrontier_BattleTower_EventScript_WarpToLobbyLost)
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_EnterElevator{
	msgbox(BattleFrontier_BattleTowerMultiPartnerRoom_Text_ThankYouForChoosingPartner, MSGBOX_DEFAULT)
	closemessage

	applymovement(ATTENDANT, Common_Movement_WalkInPlaceFasterUp)
	waitmovement(0)

	opendoor(10, 1)
	waitdooranim
	call(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_MoveToElevator)
	closedoor(10, 1)
	waitdooranim
	warp(MAP_BATTLE_FRONTIER_BATTLE_TOWER_ELEVATOR, 1, 6)

	waitstate
	releaseall
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_MoveToElevator{
	applymovement(ATTENDANT, BattleFrontier_BattleTowerMultiPartnerRoom_Movement_AttendantEnterElevator)
	applymovement(PLAYER, BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PlayerEnterElevator)
	waitmovement(0)
	return
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PlayerEnterElevator{
	walk_up
	walk_up
	set_invisible
	step_end
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_AttendantEnterElevator{
	walk_up
	set_invisible
	step_end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_TalkToPotentialPartner{
    if (flag(FLAG_CHOSEN_MULTI_BATTLE_NPC_PARTNER)){
        goto(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_RejectPartner)
    }

	tower_dopartnermsg(PARTNER_MSGID_INTRO)
	waitmessage
	waitbuttonpress

	tower_dopartnermsg(PARTNER_MSGID_MON1)
	waitmessage
	waitbuttonpress

	tower_dopartnermsg(PARTNER_MSGID_MON2_ASK)
	waitmessage

	multichoicedefault(20, 8, MULTI_YESNO, 1, FALSE)

    if (var(VAR_RESULT) != 0){
        goto(BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_RejectPartner)
    }

	tower_dopartnermsg(PARTNER_MSGID_ACCEPT)
	waitmessage
	waitbuttonpress
	closemessage

    if (var(VAR_FACING) == DIR_SOUTH){
        applymovement(VAR_LAST_TALKED,BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PartnerExitSouth)
        waitmovement(0)
    }
    applymovement(VAR_LAST_TALKED, BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PartnerExit)
    waitmovement(0)

	removeobject(VAR_LAST_TALKED)
	setflag(FLAG_CHOSEN_MULTI_BATTLE_NPC_PARTNER)
	warpsilent(MAP_BATTLE_FRONTIER_BATTLE_TOWER_MULTI_PARTNER_ROOM, 10, 3)
	waitstate
	release
	end
}

script BattleFrontier_BattleTowerMultiPartnerRoom_EventScript_RejectPartner{
	tower_dopartnermsg(PARTNER_MSGID_REJECT)
	waitmessage
	waitbuttonpress

	release
	end
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PartnerExitSouth{
	walk_fast_left
	step_end
}

movement BattleFrontier_BattleTowerMultiPartnerRoom_Movement_PartnerExit{
	walk_fast_up * 7
	step_end
}

